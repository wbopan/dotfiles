{
  "env": {
    "HTTP_PROXY": "http://127.0.0.1:7899",
    "HTTPS_PROXY": "http://127.0.0.1:7899"
  },
  "includeCoAuthoredBy": false,
  "statusLine": {
    "type": "command",
    "command": "printf '\\033[01;32m%s@%s\\033[00m:\\033[01;34m%s\\033[00m' \"$(whoami)\" \"$(hostname -s)\" \"$(pwd)\""
  },
  "alwaysThinkingEnabled": false
}
